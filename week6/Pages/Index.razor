@page "/home"

@using week6.Data.Model;
@using week6.Data
@inject HelperService HelperService

<h1>Login</h1>

<div class="form-group">
    <label>Username</label>
    <InputText id="email" class="form-control" />
</div>
<div class="form-group">
    <label>Password</label>
    <InputText id="password" type="password" class="form-control" />
</div>
<div class="form-group mt-1">
    <button type="submit" class="btn btn-primary w-100">Login Now</button>
</div>

